{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
    ],
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./public/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}